<div id="confirm_delete">
  <div class="modal-header delete">
    <h6>Confirm Delete?</h6>
  </div>
  <div class="modal-body">
    <p><%= @warning %></p>
  </div>
  <div class="modal-footer">
    <a href="/admin/<%= @klass %>/delete/<%= @identifier %>?confirm=true" class="button icon-trash">Delete</a>
    <a href="/admin/integration/cancel_dialog" class="button icon-cancel">Cancel</a>
  </div>
</div>
